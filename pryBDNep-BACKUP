                        this._compa√±√≠as_de_env√≠osTableAdapter.Adapter.AcceptChangesDuringUpdate = false;
                        adaptersWithAcceptChangesDuringUpdate.Add(this._compa√±√≠as_de_env√≠osTableAdapter.Adapter);
                    }
